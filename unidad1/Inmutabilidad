
//char
char letter = 'A';
letter = 'B';  // Ahora letter vale 'B'

//String
String greeting = "Hello";
greeting = greeting + "world";  // Ahora greeting  ahora es "Hello world" ,pero la cadena original "Hello" sigue existiendo

//COMPARACIONES

//char
char a = 'A';
char b = 'B';
boolean isEqual = (a == b);  // Devuelve false

//String
String str1 = "Java";
String str2 = "Java";
boolean isEqual = str1.equals(str2);  // Devuelve true

//ejemplo
double numeroGrande = 1.234567890123456789;
float numeroPequeno = 0.00000123f;

System.out.println(numeroGrande);
System.out.println(numeroPequeno);
